{
  "name": "react-component-example-project",
  "version": "0.1.3",
  "description": "A minimal boilerplate for building a react component for npm, including a demo page.",
  "main": "lib/index.js",
  "scripts": {
    "start": "concurrently 'npm run lib:watch' 'npm run docs'",
    "lib": "babel src/lib -d lib",
    "lib:watch": "babel src/lib -w -d lib",
    "docs": "webpack-dev-server --mode development",
    "build": "webpack -p --mode production"
  },
  "keywords": [],
  "license": "MIT",
  "peerDependencies": {
    "react": "^15.3.0 || ^16.2.0",
    "react-dom": "^15.3.0 || ^16.2.0",
    "react-helmet": "^5.2.0",
    "react-router": "^4.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.41",
    "@babel/core": "^7.0.0-beta.41",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.41",
    "@babel/preset-react": "^7.0.0-beta.41",
    "babel-loader": "^8.0.0-beta.0",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "html-webpack-plugin": "^3.2.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-helmet": "^5.2.0",
    "react-router": "^4.2.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-dev-server": "^3.1.3"
  },
